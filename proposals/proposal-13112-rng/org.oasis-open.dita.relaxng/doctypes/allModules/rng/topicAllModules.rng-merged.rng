            <define name="index-sort-as.attributes">
               <optional>
                  <attribute name="keyref"/>
               </optional>
               <ref name="univ-atts"/>
            </define>
            <define name="index-sort-as.element">
               <element name="index-sort-as" ditaarch:longName="Index Sort As">
