         <define name="relational-atts">
            <optional>
               <attribute name="type"/>
            </optional>
            <optional>
               <attribute name="format"/>
            </optional>
            <optional>
               <attribute name="scope">
                  <choice>
                     <value>external</value>
                     <value>local</value>
                     <value>peer</value>
                     <value>-dita-use-conref-target</value>
                  </choice>
               </attribute>
            </optional>
            <optional>
               <attribute name="role">
                  <choice>
                     <value>ancestor</value>
                     <value>child</value>
                     <value>cousin</value>
                     <value>descendant</value>
                     <value>external</value>
                     <value>friend</value>
                     <value>next</value>
                     <value>other</value>
                     <value>parent</value>
                     <value>previous</value>
                     <value>sample</value>
                     <value>sibling</value>
                     <value>-dita-use-conref-target</value>
