                  <ref name="univ-atts"/>
               </define>
               <define name="series.element">
                  <element name="series" ditaarch:longName="Series">
                     <a:documentation>The &lt;series&gt; metadata element contains information about the product series that the topic supports. Category: Prolog elements</a:documentation>
                     <ref name="series.attlist"/>
                     <ref name="series.content"/>
                  </element>
               </define>
               <define name="series.attlist" combine="interleave">
