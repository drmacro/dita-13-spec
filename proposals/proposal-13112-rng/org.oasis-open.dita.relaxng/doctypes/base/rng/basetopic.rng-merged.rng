      <choice>
         <text/>
         <ref name="basic.block"/>
         <ref name="basic.ph"/>
         <ref name="data.elements.incl"/>
         <ref name="foreign.unknown.incl"/>
         <ref name="sectiondiv"/>
         <ref name="title"/>
         <ref name="txt.incl"/>
      </choice>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="section.notitle.cnt">
      <choice>
         <text/>
         <ref name="basic.block"/>
         <ref name="basic.ph"/>
         <ref name="data.elements.incl"/>
         <ref name="foreign.unknown.incl"/>
         <ref name="sectiondiv"/>
         <ref name="txt.incl"/>
      </choice>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="sectiondiv.cnt">
      <a:documentation>sectiondiv also includes sectiondiv</a:documentation>
      <choice>
         <text/>
         <ref name="basic.block"/>
         <ref name="basic.ph"/>
         <ref name="data.elements.incl"/>
         <ref name="foreign.unknown.incl"/>
         <ref name="txt.incl"/>
      </choice>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="relational-atts">
      <optional>
         <attribute name="type"/>
      </optional>
      <optional>
         <attribute name="format"/>
      </optional>
      <optional>
         <attribute name="scope">
            <choice>
               <value>external</value>
               <value>local</value>
               <value>peer</value>
               <value>-dita-use-conref-target</value>
            </choice>
         </attribute>
      </optional>
      <optional>
         <attribute name="role">
            <choice>
               <value>ancestor</value>
               <value>child</value>
               <value>cousin</value>
               <value>descendant</value>
               <value>external</value>
               <value>friend</value>
               <value>next</value>
               <value>other</value>
               <value>parent</value>
               <value>previous</value>
               <value>sample</value>
               <value>sibling</value>
               <value>-dita-use-conref-target</value>
            </choice>
         </attribute>
      </optional>
      <optional>
         <attribute name="otherrole"/>
      </optional>
   </define>
   <a:documentation xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/">ELEMENT TYPE DECLARATIONS</a:documentation>
   <a:documentation xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/">LONG NAME: Topic</a:documentation>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="topic.content">
      <ref name="title"/>
      <optional>
         <ref name="titlealts"/>
      </optional>
      <optional>
         <choice>
            <ref name="shortdesc"/>
            <ref name="abstract"/>
         </choice>
      </optional>
      <optional>
         <ref name="prolog"/>
      </optional>
      <optional>
         <ref name="body"/>
      </optional>
      <optional>
         <ref name="related-links"/>
      </optional>
      <zeroOrMore>
         <ref name="topic-info-types"/>
      </zeroOrMore>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="topic.attributes">
      <attribute name="id">
         <data type="ID"/>
      </attribute>
      <ref name="conref-atts"/>
      <ref name="select-atts"/>
      <ref name="localization-atts"/>
      <optional>
         <attribute name="outputclass"/>
      </optional>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="topic.element">
      <element name="topic" a:longName="Topic">
         <a:documentation>The &lt;topic&gt; element is the top-level DITA element for a single-subject topic or article. Other top-level DITA elements that are more content-specific are &lt;concept&gt;,
            &lt;task&gt;, &lt;reference&gt;, and &lt;glossary&gt;. Category: Topic elements</a:documentation>
         <ref name="topic.attlist"/>
         <ref name="topic.content"/>
      </element>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="topic.attlist"
           combine="interleave">
      <ref name="topic.attributes"/>
      <ref name="arch-atts"/>
      <ref name="domains-att"/>
   </define>
   <a:documentation xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/">LONG NAME: Title Alternatives</a:documentation>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="titlealts.content">
      <optional>
         <ref name="navtitle"/>
      </optional>
      <optional>
         <ref name="searchtitle"/>
      </optional>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="titlealts.attributes">
      <ref name="univ-atts"/>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="titlealts.element">
      <element name="titlealts" a:longName="Title Alternatives">
         <a:documentation>The alternate title element (&lt;titlealts&gt;) is optional, but can occur after the topic title. Two elements can be inserted as sub-elements of &lt;titlealts&gt;: navigation
            title &lt;navtitle&gt; and search title &lt;searchtitle&gt;. Category: Topic elements</a:documentation>
         <ref name="titlealts.attlist"/>
         <ref name="titlealts.content"/>
      </element>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="titlealts.attlist"
           combine="interleave">
      <ref name="titlealts.attributes"/>
   </define>
   <a:documentation xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/">LONG NAME: Search Title</a:documentation>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="searchtitle.content">
      <zeroOrMore>
         <choice>
            <ref name="words.cnt"/>
            <ref name="ph"/>
         </choice>
      </zeroOrMore>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="searchtitle.attributes">
      <ref name="univ-atts"/>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="searchtitle.element">
      <element name="searchtitle" a:longName="Search Title">
         <a:documentation>When your DITA topic is transformed to XHTML, the &lt;searchtitle&gt; element is used to create a title element at the top of the resulting HTML file. This title is normally
            used in search result summaries by some search engines, such as that in Eclipse (http://eclipse.org); if not set, the XHTML's title element defaults to the source topic's title content
            (which may not be as well optimized for search summaries) Category: Topic elements</a:documentation>
         <ref name="searchtitle.attlist"/>
         <ref name="searchtitle.content"/>
      </element>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="searchtitle.attlist"
           combine="interleave">
      <ref name="searchtitle.attributes"/>
   </define>
   <a:documentation xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/">LONG NAME: Abstract</a:documentation>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="abstract.content">
      <zeroOrMore>
         <ref name="abstract.cnt"/>
      </zeroOrMore>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="abstract.attributes">
      <ref name="univ-atts"/>
      <optional>
         <attribute name="outputclass"/>
      </optional>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="abstract.element">
      <element name="abstract" a:longName="Abstract">
         <a:documentation>The abstract element occurs between the topic title and the topic body, as the initial content of a topic. It can contain paragraph-level content as well as one or more
            shortdesc elements which can be used for providing link previews or summaries. The &lt;abstract&gt; element cannot be overridden by maps, but its contained &lt;shortdesc&gt; elements can be, for
            the purpose of creating link summaries or previews. Category: Topic elements</a:documentation>
         <ref name="abstract.attlist"/>
         <ref name="abstract.content"/>
      </element>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="abstract.attlist"
           combine="interleave">
      <ref name="abstract.attributes"/>
   </define>
   <a:documentation xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/">LONG NAME: Body</a:documentation>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="body.content">
      <zeroOrMore>
         <choice>
            <ref name="body.cnt"/>
            <ref name="bodydiv"/>
            <ref name="example"/>
            <ref name="section"/>
         </choice>
      </zeroOrMore>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="body.attributes">
      <ref name="univ-atts"/>
      <optional>
         <attribute name="outputclass"/>
      </optional>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="body.element">
      <element name="body" a:longName="Body">
         <a:documentation>The &lt;body&gt; element is the container for the main content of a &lt;topic&gt;. Category: Topic elements</a:documentation>
         <ref name="body.attlist"/>
         <ref name="body.content"/>
      </element>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="body.attlist"
           combine="interleave">
      <ref name="body.attributes"/>
   </define>
   <a:documentation xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/">LONG NAME: Body division</a:documentation>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="bodydiv.content">
      <zeroOrMore>
         <choice>
            <ref name="bodydiv.cnt"/>
            <ref name="bodydiv"/>
            <ref name="section"/>
         </choice>
      </zeroOrMore>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="bodydiv.attributes">
      <ref name="univ-atts"/>
      <optional>
         <attribute name="outputclass"/>
      </optional>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="bodydiv.element">
      <element name="bodydiv" a:longName="Body division">
         <a:documentation>The &lt;bodydiv&gt; element is used to contain informal blocks of information within the body of a topic. The bodydiv element is specifically designed to be a grouping element,
            without any explicit semantics, other than to organize subsets of content into logical groups that are not intended or should not be contained as a topic. As such, it does not contain an
            explicit title to avoid enabling the creation of deeply nested content that would otherwise be written as separate topics. Content that requires a title should use a section element or a
            nested topic.</a:documentation>
         <ref name="bodydiv.attlist"/>
         <ref name="bodydiv.content"/>
      </element>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="bodydiv.attlist"
           combine="interleave">
      <ref name="bodydiv.attributes"/>
   </define>
   <a:documentation xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/">LONG NAME: No Topic nesting</a:documentation>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="no-topic-nesting.element">
      <element name="no-topic-nesting" a:longName="No Topic nesting">
         <a:documentation>The &lt;no-topic-nesting&gt; element is a placeholder in the DITA architecture. It is not actually used by the default DITA document types; it is for use only when creating a
            validly customized document type where the information designer wants to eliminate the ability to nest topics. Not intended for use by authors, and has no associated output processing.
            Category: Specialization elements</a:documentation>
         <ref name="no-topic-nesting.attlist"/>
         <empty/>
      </element>
   </define>
   <a:documentation xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/">LONG NAME: Section</a:documentation>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="section.content">
      <zeroOrMore>
         <ref name="section.cnt"/>
      </zeroOrMore>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="section.attributes">
      <optional>
         <attribute name="spectitle"/>
      </optional>
      <ref name="univ-atts"/>
      <optional>
         <attribute name="outputclass"/>
      </optional>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="section.element">
      <element name="section" a:longName="Section">
         <a:documentation>The &lt;section&gt; element represents an organizational division in a topic. Sections are used to organize subsets of information that are directly related to the topic. For
            example, the titles Reference Syntax, Example and Properties might represent section-level discourse within a topic about a command-line processthe content in each section relates
            uniquely to the subject of that topic. Multiple sections within a single topic do not represent a hierarchy, but rather peer divisions of that topic. Sections cannot be nested. A section
            may have an optional title. Category: Topic elements</a:documentation>
         <ref name="section.attlist"/>
         <ref name="section.content"/>
      </element>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="section.attlist"
           combine="interleave">
      <ref name="section.attributes"/>
   </define>
   <a:documentation xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/">LONG NAME: Section division</a:documentation>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="sectiondiv.content">
      <zeroOrMore>
         <choice>
            <ref name="sectiondiv.cnt"/>
            <ref name="sectiondiv"/>
         </choice>
      </zeroOrMore>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="sectiondiv.attributes">
      <ref name="univ-atts"/>
      <optional>
         <attribute name="outputclass"/>
      </optional>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="sectiondiv.element">
      <element name="sectiondiv" a:longName="Section division">
         <a:documentation>The &lt;sectiondiv&gt; element allows logical grouping of content within a section. There is no additional semantic associated with the sectiondiv element, aside from its
            function as a container for other content. The sectiondiv element does not contain a title; the lowest level of titled content within a topic is the section itself. If additional hierarchy
            is required, nested topics should be used in place of the section.</a:documentation>
         <ref name="sectiondiv.attlist"/>
         <ref name="sectiondiv.content"/>
      </element>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="sectiondiv.attlist"
           combine="interleave">
      <ref name="sectiondiv.attributes"/>
   </define>
   <a:documentation xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/">LONG NAME: Example</a:documentation>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="example.content">
      <zeroOrMore>
         <ref name="example.cnt"/>
      </zeroOrMore>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="example.attributes">
      <optional>
         <attribute name="spectitle"/>
      </optional>
      <ref name="univ-atts"/>
      <optional>
         <attribute name="outputclass"/>
      </optional>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="example.element">
      <element name="example" a:longName="Example">
         <a:documentation>The &lt;example&gt; element is a section with the specific role of containing examples that illustrate or support the current topic. The &lt;example&gt; element has the same
            content model as &lt;section&gt;. Category: Topic elements</a:documentation>
         <ref name="example.attlist"/>
         <ref name="example.content"/>
      </element>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="example.attlist"
           combine="interleave">
      <ref name="example.attributes"/>
   </define>
   <a:documentation xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/">PROLOG (METADATA FOR TOPICS)</a:documentation>
   <a:documentation xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/">LONG NAME: Prolog</a:documentation>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="prolog.content">
      <zeroOrMore>
         <ref name="author"/>
      </zeroOrMore>
      <optional>
         <ref name="source"/>
      </optional>
      <optional>
         <ref name="publisher"/>
      </optional>
      <zeroOrMore>
         <ref name="copyright"/>
      </zeroOrMore>
      <optional>
         <ref name="critdates"/>
      </optional>
      <optional>
         <ref name="permissions"/>
      </optional>
      <zeroOrMore>
         <ref name="metadata"/>
      </zeroOrMore>
      <zeroOrMore>
         <ref name="resourceid"/>
      </zeroOrMore>
      <zeroOrMore>
         <choice>
            <ref name="data.elements.incl"/>
            <ref name="foreign.unknown.incl"/>
         </choice>
      </zeroOrMore>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="prolog.attributes">
      <ref name="univ-atts"/>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="prolog.element">
      <element name="prolog" a:longName="prolog">
         <a:documentation>The &lt;prolog&gt; element contains information about the topic as an whole (for example, author information or subject category) that is either entered by the author or
              machine-maintained. Much of the metadata inside the &lt;prolog&gt; will not be displayed with the topic on output, but may be used by processes that generate search indexes or customize
              navigation. Category: Prolog elements</a:documentation>
         <ref name="prolog.attlist"/>
         <ref name="prolog.content"/>
      </element>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="prolog.attlist"
           combine="interleave">
      <ref name="prolog.attributes"/>
   </define>
   <a:documentation xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/">LINKING GROUPING</a:documentation>
   <a:documentation xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/">LONG NAME: Related Links</a:documentation>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="related-links.content">
      <zeroOrMore>
         <choice>
            <ref name="link"/>
            <ref name="linklist"/>
            <ref name="linkpool"/>
         </choice>
      </zeroOrMore>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="related-links.attributes">
      <ref name="relational-atts"/>
      <ref name="univ-atts"/>
      <optional>
         <attribute name="outputclass"/>
      </optional>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="related-links.element">
      <element name="related-links" a:longName="related-links">
         <a:documentation>The related information links of a topic (&lt;related-links&gt; element) are stored in a special section following the body of the topic. After a topic is processed into it
              final output form, the related links are usually displayed at the end of the topic, although some Web-based help systems might display them in a separate navigation frame. Category:
              Topic elements</a:documentation>
         <ref name="related-links.attlist"/>
         <ref name="related-links.content"/>
      </element>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="related-links.attlist"
           combine="interleave">
      <ref name="related-links.attributes"/>
   </define>
   <a:documentation xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/">LONG NAME: Link</a:documentation>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="link.content">
      <optional>
         <ref name="linktext"/>
      </optional>
      <optional>
         <ref name="desc"/>
      </optional>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="link.attributes">
      <optional>
         <attribute name="href"/>
      </optional>
      <optional>
         <attribute name="keyref"/>
      </optional>
      <optional>
         <attribute name="query"/>
      </optional>
      <ref name="relational-atts"/>
      <ref name="univ-atts"/>
      <optional>
         <attribute name="outputclass"/>
      </optional>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="link.element">
      <element name="link" a:longName="link">
         <a:documentation>The &lt;link&gt; element defines a relationship to another topic. Links represent the types and roles of topics in a web of information, and therefore represent navigational
              links within that web. Links are typically sorted on output based on their attributes. The optional container elements for link (&lt;linkpool&gt; and &lt;linklist&gt;) allow authors to define
              groups with common attributes, or to preserve the authored sequence of links on output. Links placed in a &lt;linkpool&gt; may be rearranged for display purposes (combined with other local
              or map-based links); links in a &lt;linklist&gt; should be displayed in the order they are defined. Refer to those elements for additional explanation. Category: Related Links
              elements</a:documentation>
         <ref name="link.attlist"/>
         <ref name="link.content"/>
      </element>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="link.attlist"
           combine="interleave">
      <ref name="link.attributes"/>
   </define>
   <a:documentation xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/">LONG NAME: Link Text</a:documentation>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="linktext.content">
      <zeroOrMore>
         <choice>
            <ref name="words.cnt"/>
            <ref name="ph"/>
         </choice>
      </zeroOrMore>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="linktext.attributes">
      <ref name="univ-atts"/>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="linktext.element">
      <element name="linktext" a:longName="linktext">
         <a:documentation>The &lt;linktext&gt; element provides the literal label or line of text for a link. In most cases, the text of a link can be resolved during processing by cross reference
              with the target resource. Use the &lt;linktext&gt; element only when the target cannot be reached, such as when it is a peer or external link, or the target is local but not in DITA format.
              When used inside a topic, it will be used as the text for the specified link; when used within a map, it will be used as the text for generated links that point to the specified topic.
              Category: Related Links elements</a:documentation>
         <ref name="linktext.attlist"/>
         <ref name="linktext.content"/>
      </element>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="linktext.attlist"
           combine="interleave">
      <ref name="linktext.attributes"/>
   </define>
   <a:documentation xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/">LONG NAME: Link List</a:documentation>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="linklist.content">
      <optional>
         <ref name="title"/>
      </optional>
      <optional>
         <ref name="desc"/>
      </optional>
      <zeroOrMore>
         <choice>
            <ref name="linklist"/>
            <ref name="link"/>
         </choice>
      </zeroOrMore>
      <optional>
         <ref name="linkinfo"/>
      </optional>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="linklist.attributes">
      <optional>
         <attribute name="collection-type">
            <choice>
               <value>choice</value>
               <value>family</value>
               <value>sequence</value>
               <value>tree</value>
               <value>unordered</value>
               <value>-dita-use-conref-target</value>
            </choice>
         </attribute>
      </optional>
      <optional>
         <attribute name="duplicates">
            <choice>
               <value>no</value>
               <value>yes</value>
               <value>-dita-use-conref-target</value>
            </choice>
         </attribute>
      </optional>
      <optional>
         <attribute name="mapkeyref"/>
      </optional>
      <ref name="relational-atts"/>
      <ref name="univ-atts"/>
      <optional>
         <attribute name="spectitle"/>
      </optional>
      <optional>
         <attribute name="outputclass"/>
      </optional>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="linklist.element">
      <element name="linklist" a:longName="linklist">
         <a:documentation>The &lt;linklist&gt; element defines an author-arranged group of links. Within &lt;linklist&gt;, the organization of links on final output is in the same order as originally
              authored in the DITA topic. Category: Related Links elements</a:documentation>
         <ref name="linklist.attlist"/>
         <ref name="linklist.content"/>
      </element>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="linklist.attlist"
           combine="interleave">
      <ref name="linklist.attributes"/>
   </define>
   <a:documentation xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/">LONG NAME: Link Information</a:documentation>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="linkinfo.content">
      <zeroOrMore>
         <ref name="desc.cnt"/>
      </zeroOrMore>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="linkinfo.attributes">
      <ref name="univ-atts"/>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="linkinfo.element">
      <element name="linkinfo" a:longName="linkinfo">
         <a:documentation>The &lt;linkinfo&gt; element allows you to place a descriptive paragraph following a list of links in a &lt;linklist&gt; element. Category: Related Links
              elements</a:documentation>
         <ref name="linkinfo.attlist"/>
         <ref name="linkinfo.content"/>
      </element>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="linkinfo.attlist"
           combine="interleave">
      <ref name="linkinfo.attributes"/>
   </define>
   <a:documentation xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/">LONG NAME: Link Pool</a:documentation>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="linkpool.content">
      <zeroOrMore>
         <choice>
            <ref name="linkpool"/>
            <ref name="link"/>
         </choice>
      </zeroOrMore>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="linkpool.attributes">
      <optional>
         <attribute name="collection-type">
            <choice>
               <value>choice</value>
               <value>family</value>
               <value>sequence</value>
               <value>tree</value>
               <value>unordered</value>
               <value>-dita-use-conref-target</value>
            </choice>
         </attribute>
      </optional>
      <optional>
         <attribute name="duplicates">
            <choice>
               <value>no</value>
               <value>yes</value>
               <value>-dita-use-conref-target</value>
            </choice>
         </attribute>
      </optional>
      <optional>
         <attribute name="mapkeyref"/>
      </optional>
      <ref name="relational-atts"/>
      <ref name="univ-atts"/>
      <optional>
         <attribute name="outputclass"/>
      </optional>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="linkpool.element">
      <element name="linkpool" a:longName="linkpool">
         <a:documentation>The &lt;linkpool&gt; element defines a group of links that have common characteristics, such as type or audience or source. When links are not in a &lt;linklist&gt; (that is,
              they are in &lt;related-links&gt; or &lt;linkpool&gt; elements), the organization of links on final output is determined by the output process, not by the order that the links actually occur
              in the DITA topic. Category: Related Links elements</a:documentation>
         <ref name="linkpool.attlist"/>
         <ref name="linkpool.content"/>
      </element>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="linkpool.attlist"
           combine="interleave">
      <ref name="linkpool.attributes"/>
   </define>
