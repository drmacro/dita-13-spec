   <a:documentation xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/">DOMAIN EXTENSION PATTERNS</a:documentation>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="hi-d-ph">
      <choice>
         <ref name="b"/>
         <ref name="i"/>
         <ref name="sup"/>
         <ref name="sub"/>
         <ref name="tt"/>
         <ref name="u"/>
      </choice>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="ph"
           combine="choice">
      <ref name="hi-d-ph"/>
   </define>
   <a:documentation xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/">ELEMENT TYPE NAME PATTERNS</a:documentation>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/" name="b">
      <ref name="b.element"/>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/" name="i">
      <ref name="i.element"/>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/" name="sup">
      <ref name="sup.element"/>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/" name="sub">
      <ref name="sub.element"/>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/" name="tt">
      <ref name="tt.element"/>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/" name="u">
      <ref name="u.element"/>
   </define>
   <a:documentation xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/">ELEMENT TYPE DECLARATIONS</a:documentation>
   <a:documentation xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/">LONG NAME: Bold</a:documentation>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="b.content">
      <zeroOrMore>
         <choice>
            <text/>
            <ref name="basic.ph"/>
            <ref name="data.elements.incl"/>
            <ref name="foreign.unknown.incl"/>
         </choice>
      </zeroOrMore>
   </define>
   <define xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
           name="b.attributes">
      <ref name="univ-atts"/>
      <optional>
         <attribute name="outputclass"/>
