   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="dl.attributes">
      <optional>
         <attribute name="compact">
            <choice>
               <value>no</value>
               <value>yes</value>
               <value>-dita-use-conref-target</value>
            </choice>
         </attribute>
      </optional>
      <optional>
         <attribute name="spectitle"/>
      </optional>
      <ref name="univ-atts"/>
      <optional>
         <attribute name="outputclass"/>
      </optional>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="dl.element">
      <element name="dl" a:longName="Definition List">
         <a:documentation>A definition list (&lt;dl&gt;) is a list of terms and corresponding definitions. The term (&lt;dt&gt;) is usually flush left. The description or definition (&lt;dd&gt;) is usually
              either indented and on the next line, or on the same line to the right of the term. Category: Body elements</a:documentation>
         <ref name="dl.attlist"/>
         <ref name="dl.content"/>
      </element>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron"
           name="dl.attlist"
           combine="interleave">
      <ref name="dl.attributes"/>
   </define>
   <a:documentation xmlns:sch="http://purl.oclc.org/dsdl/schematron">LONG NAME: Definition List Head</a:documentation>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="dlhead.content">
      <optional>
         <ref name="dthd"/>
      </optional>
      <optional>
         <ref name="ddhd"/>
      </optional>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="dlhead.attributes">
      <ref name="univ-atts"/>
      <optional>
         <attribute name="outputclass"/>
      </optional>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="dlhead.element">
      <element name="dlhead" a:longName="Definition List Head">
         <a:documentation>The &lt;dlhead&gt; element contains optional headings for the term and description columns in a definition list. The definition list heading contains a heading &lt;dthd&gt; for
              the column of terms and an optional heading &lt;ddhd&gt;for the column of descriptions. Category: Body elements</a:documentation>
         <ref name="dlhead.attlist"/>
         <ref name="dlhead.content"/>
      </element>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron"
           name="dlhead.attlist"
           combine="interleave">
      <ref name="dlhead.attributes"/>
   </define>
   <a:documentation xmlns:sch="http://purl.oclc.org/dsdl/schematron">LONG NAME: Term Header</a:documentation>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="dthd.content">
      <zeroOrMore>
         <ref name="title.cnt"/>
      </zeroOrMore>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="dthd.attributes">
      <ref name="univ-atts"/>
      <optional>
         <attribute name="outputclass"/>
      </optional>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="dthd.element">
      <element name="dthd" a:longName="Term Header">
         <a:documentation>The definition term heading (&lt;dthd&gt;) element is contained in a definition list head (&lt;dlhead&gt;) and provides an optional heading for the column of terms in a
              description list. Category: Body elements</a:documentation>
         <ref name="dthd.attlist"/>
         <ref name="dthd.content"/>
      </element>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron"
           name="dthd.attlist"
           combine="interleave">
      <ref name="dthd.attributes"/>
   </define>
   <a:documentation xmlns:sch="http://purl.oclc.org/dsdl/schematron">LONG NAME: Definition Header</a:documentation>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="ddhd.content">
      <zeroOrMore>
         <ref name="title.cnt"/>
      </zeroOrMore>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="ddhd.attributes">
      <ref name="univ-atts"/>
      <optional>
         <attribute name="outputclass"/>
      </optional>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="ddhd.element">
      <element name="ddhd" a:longName="Definition Header">
         <a:documentation>The definition descriptions heading (&lt;ddhd&gt;) element contains an optional heading or title for a column of descriptions or definitions in a definition list. Category:
              Body elements</a:documentation>
         <ref name="ddhd.attlist"/>
         <ref name="ddhd.content"/>
      </element>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron"
           name="ddhd.attlist"
           combine="interleave">
      <ref name="ddhd.attributes"/>
   </define>
   <a:documentation xmlns:sch="http://purl.oclc.org/dsdl/schematron">LONG NAME: Definition List Entry</a:documentation>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="dlentry.content">
      <oneOrMore>
         <ref name="dt"/>
      </oneOrMore>
      <oneOrMore>
         <ref name="dd"/>
      </oneOrMore>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="dlentry.attributes">
      <ref name="univ-atts"/>
      <optional>
         <attribute name="outputclass"/>
      </optional>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="dlentry.element">
      <element name="dlentry" a:longName="Definition List Entry">
         <a:documentation>In a definition list, each list item is defined by the definition list entry (&lt;dlentry&gt;) element. The definition list entry element includes a term &lt;dt&gt; and one or
              more definitions or descriptions &lt;dd&gt; of that term. Category: Body elements</a:documentation>
         <ref name="dlentry.attlist"/>
         <ref name="dlentry.content"/>
      </element>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron"
           name="dlentry.attlist"
           combine="interleave">
      <ref name="dlentry.attributes"/>
   </define>
   <a:documentation xmlns:sch="http://purl.oclc.org/dsdl/schematron">LONG NAME: Definition Term</a:documentation>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="dt.content">
      <zeroOrMore>
         <ref name="term.cnt"/>
      </zeroOrMore>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="dt.attributes">
      <optional>
         <attribute name="keyref"/>
      </optional>
      <ref name="univ-atts"/>
      <optional>
         <attribute name="outputclass"/>
      </optional>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="dt.element">
      <element name="dt" a:longName="Definition Term">
         <a:documentation>The definition term &lt;dt&gt; element contains a term in a definition list entry. Category: Body elements</a:documentation>
         <ref name="dt.attlist"/>
         <ref name="dt.content"/>
      </element>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron"
           name="dt.attlist"
           combine="interleave">
      <ref name="dt.attributes"/>
   </define>
   <a:documentation xmlns:sch="http://purl.oclc.org/dsdl/schematron">LONG NAME: Definition Description</a:documentation>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="dd.content">
      <zeroOrMore>
         <ref name="defn.cnt"/>
      </zeroOrMore>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="dd.attributes">
      <ref name="univ-atts"/>
      <optional>
         <attribute name="outputclass"/>
      </optional>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="dd.element">
      <element name="dd" a:longName="Definition Description">
         <a:documentation>The definition description (&lt;dd&gt;) element contains the description of a term in a definition list entry. Category: Body elements</a:documentation>
         <ref name="dd.attlist"/>
         <ref name="dd.content"/>
      </element>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron"
           name="dd.attlist"
           combine="interleave">
      <ref name="dd.attributes"/>
   </define>
   <a:documentation xmlns:sch="http://purl.oclc.org/dsdl/schematron">LONG NAME: Figure</a:documentation>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="fig.content">
      <optional>
         <ref name="title"/>
      </optional>
      <optional>
         <ref name="desc"/>
      </optional>
      <zeroOrMore>
         <choice>
            <ref name="figgroup"/>
            <ref name="fig.cnt"/>
         </choice>
      </zeroOrMore>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="fig.attributes">
      <ref name="display-atts"/>
      <optional>
         <attribute name="spectitle"/>
      </optional>
      <ref name="univ-atts"/>
      <optional>
         <attribute name="outputclass"/>
      </optional>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="fig.element">
      <element name="fig" a:longName="Figure">
         <a:documentation>The figure (&lt;fig&gt;) element is a display context (sometimes called an exhibit) with an optional title for a wide variety of content. Most commonly, the figure element
              contains an image element (a graphic or artwork), but it can contain several kinds of text objects as well. A title is placed inside the figure element to provide a caption to describe
              the content. Category: Body elements</a:documentation>
         <ref name="fig.attlist"/>
         <ref name="fig.content"/>
      </element>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron"
           name="fig.attlist"
           combine="interleave">
      <ref name="fig.attributes"/>
   </define>
   <a:documentation xmlns:sch="http://purl.oclc.org/dsdl/schematron">LONG NAME: Figure Group</a:documentation>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="figgroup.content">
      <optional>
         <ref name="title"/>
      </optional>
      <zeroOrMore>
         <choice>
            <ref name="figgroup"/>
            <ref name="figgroup.cnt"/>
         </choice>
      </zeroOrMore>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="figgroup.attributes">
      <ref name="univ-atts"/>
      <optional>
         <attribute name="outputclass"/>
      </optional>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="figgroup.element">
      <element name="figgroup" a:longName="Figure Group">
         <a:documentation>The &lt;figgroup&gt; element is used only for specialization at this time. Figure groups can be used to contain multiple cross-references, footnotes or keywords, but not
              multipart images. Multipart images in DITA should be represented by a suitable media type displayed by the &lt;object&gt; element. Category: Body elements</a:documentation>
         <ref name="figgroup.attlist"/>
         <ref name="figgroup.content"/>
      </element>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron"
           name="figgroup.attlist"
           combine="interleave">
      <ref name="figgroup.attributes"/>
   </define>
   <a:documentation xmlns:sch="http://purl.oclc.org/dsdl/schematron">LONG NAME: Preformatted Text</a:documentation>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="pre.content">
      <zeroOrMore>
         <ref name="pre.cnt"/>
      </zeroOrMore>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="pre.attributes">
      <ref name="display-atts"/>
      <optional>
         <attribute name="spectitle"/>
      </optional>
      <optional>
         <attribute name="xml:space" a:defaultValue="preserve">
            <value>preserve</value>
         </attribute>
      </optional>
      <ref name="univ-atts"/>
      <optional>
         <attribute name="outputclass"/>
      </optional>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="pre.element">
      <element name="pre" a:longName="Preformatted Text">
         <a:documentation>The preformatted element (&lt;pre&gt;) preserves line breaks and spaces entered manually by the author in the content of the element, and also presents the content in a
              monospaced type font (depending on your output formatting processor). Do not use &lt;pre&gt; when a more semantically specific element is appropriate, such as &lt;codeblock&gt;. Category: Body
              elements</a:documentation>
         <ref name="pre.attlist"/>
         <ref name="pre.content"/>
      </element>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron"
           name="pre.attlist"
           combine="interleave">
      <ref name="pre.attributes"/>
   </define>
   <a:documentation xmlns:sch="http://purl.oclc.org/dsdl/schematron">LONG NAME: Line Respecting Text</a:documentation>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="lines.content">
      <zeroOrMore>
         <ref name="pre.cnt"/>
      </zeroOrMore>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="lines.attributes">
      <ref name="display-atts"/>
      <optional>
         <attribute name="spectitle"/>
      </optional>
      <optional>
         <attribute name="xml:space" a:defaultValue="preserve">
            <value>preserve</value>
         </attribute>
      </optional>
      <ref name="univ-atts"/>
      <optional>
         <attribute name="outputclass"/>
      </optional>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="lines.element">
      <element name="lines" a:longName="Line Respecting Text">
         <a:documentation>The &lt;lines&gt; element may be used to represent dialogs, lists, text fragments, and so forth. The &lt;lines&gt; element is similar to &lt;pre&gt; in that hard line breaks are
              preserved, but the font style is not set to monospace, and extra spaces inside the lines are not preserved. Category: Body elements</a:documentation>
         <ref name="lines.attlist"/>
         <ref name="lines.content"/>
      </element>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron"
           name="lines.attlist"
           combine="interleave">
      <ref name="lines.attributes"/>
   </define>
   <a:documentation xmlns:sch="http://purl.oclc.org/dsdl/schematron"> BASE FORM PHRASE TYPES</a:documentation>
   <a:documentation xmlns:sch="http://purl.oclc.org/dsdl/schematron">LONG NAME: Text</a:documentation>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="text.content">
      <zeroOrMore>
         <choice>
            <text/>
            <ref name="text"/>
         </choice>
      </zeroOrMore>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="text.attributes">
      <ref name="univ-atts"/>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="text.element">
      <element name="text" a:longName="Text">
         <a:documentation>The text element associates no semantics with its content. It exists to serve as a container for text where a container is needed (e.g., for conref, or for restricted
              content models in specializations). Unlike ph, text cannot contain images. Unlike keyword, text does not imply keyword-like semantics. The text element contains only text data, or nested
              text elements. All universal attributes are available on text.</a:documentation>
         <ref name="text.attlist"/>
         <ref name="text.content"/>
      </element>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron"
           name="text.attlist"
           combine="interleave">
      <ref name="text.attributes"/>
   </define>
   <a:documentation xmlns:sch="http://purl.oclc.org/dsdl/schematron">LONG NAME: Keyword</a:documentation>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="keyword.content">
      <zeroOrMore>
         <choice>
            <text/>
            <ref name="text"/>
            <ref name="tm"/>
         </choice>
      </zeroOrMore>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="keyword.attributes">
      <optional>
         <attribute name="keyref"/>
      </optional>
      <ref name="univ-atts"/>
      <optional>
         <attribute name="outputclass"/>
      </optional>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="keyword.element">
      <element name="keyword" a:longName="Keyword">
         <a:documentation>The &lt;keyword&gt; element identifies a keyword or token, such as a single value from an enumerated list, the name of a command or parameter, product name, or a lookup key
              for a message. Category: Body elements</a:documentation>
         <ref name="keyword.attlist"/>
         <ref name="keyword.content"/>
      </element>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron"
           name="keyword.attlist"
           combine="interleave">
      <ref name="keyword.attributes"/>
   </define>
   <a:documentation xmlns:sch="http://purl.oclc.org/dsdl/schematron">LONG NAME: Term</a:documentation>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="term.content">
      <zeroOrMore>
         <choice>
            <text/>
            <ref name="text"/>
            <ref name="tm"/>
         </choice>
      </zeroOrMore>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="term.attributes">
      <optional>
         <attribute name="keyref"/>
      </optional>
      <ref name="univ-atts"/>
      <optional>
         <attribute name="outputclass"/>
      </optional>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="term.element">
      <element name="term" a:longName="Term">
         <a:documentation>The &lt;term&gt; element identifies words that may have or require extended definitions or explanations. In future development of DITA, for example, terms might provide
              associative linking to matching glossary entries. Category: Specialization elements</a:documentation>
         <ref name="term.attlist"/>
         <ref name="term.content"/>
      </element>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron"
           name="term.attlist"
           combine="interleave">
      <ref name="term.attributes"/>
   </define>
   <a:documentation xmlns:sch="http://purl.oclc.org/dsdl/schematron">LONG NAME: Phrase</a:documentation>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="ph.content">
      <zeroOrMore>
         <choice>
            <ref name="ph.cnt"/>
            <ref name="text"/>
         </choice>
      </zeroOrMore>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="ph.attributes">
      <optional>
         <attribute name="keyref"/>
      </optional>
      <ref name="univ-atts"/>
      <optional>
         <attribute name="outputclass"/>
      </optional>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="ph.element">
      <element name="ph" a:longName="Phrase">
         <a:documentation>The phrase (&lt;ph&gt;) element is used to organize content for reuse or conditional processing (for example, when part of a paragraph applies to a particular audience). It
              can be used by specializations of DITA to create semantic markup for content at the phrase level, which then allows (but does not require) specific processing or formatting. Category:
              Body elements</a:documentation>
         <ref name="ph.attlist"/>
         <ref name="ph.content"/>
      </element>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron"
           name="ph.attlist"
           combine="interleave">
      <ref name="ph.attributes"/>
   </define>
   <a:documentation xmlns:sch="http://purl.oclc.org/dsdl/schematron">LONG NAME: Trade Mark</a:documentation>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="tm.content">
      <zeroOrMore>
         <choice>
            <text/>
            <ref name="text"/>
            <ref name="tm"/>
         </choice>
      </zeroOrMore>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="tm.attributes">
      <optional>
         <attribute name="trademark"/>
      </optional>
      <optional>
         <attribute name="tmowner"/>
      </optional>
      <attribute name="tmtype">
         <choice>
            <value>reg</value>
            <value>service</value>
            <value>tm</value>
            <value>-dita-use-conref-target</value>
         </choice>
      </attribute>
      <optional>
         <attribute name="tmclass"/>
      </optional>
      <ref name="univ-atts"/>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="tm.element">
      <element name="tm" a:longName="Trade Mark">
         <a:documentation>The trademark (&lt;tm&gt;) element in DITA is used to markup and identify a term or phrase that is trademarked. Trademarks include registered trademarks, service marks,
              slogans and logos. Category: Miscellaneous elements</a:documentation>
         <ref name="tm.attlist"/>
         <ref name="tm.content"/>
      </element>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron"
           name="tm.attlist"
           combine="interleave">
      <ref name="tm.attributes"/>
   </define>
   <a:documentation xmlns:sch="http://purl.oclc.org/dsdl/schematron">LONG NAME: Boolean (deprecated)</a:documentation>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="boolean.content">
      <empty/>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="boolean.attributes">
      <attribute name="state">
         <choice>
            <value>no</value>
            <value>yes</value>
            <value>-dita-use-conref-target</value>
         </choice>
      </attribute>
      <ref name="univ-atts"/>
      <optional>
         <attribute name="outputclass"/>
      </optional>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="boolean.element">
      <element name="boolean" a:longName="Boolean  (deprecated)">
         <a:documentation>The &lt;boolean&gt; element is used to express one of two opposite values, such as yes or no, on or off, true or false, high or low, and so forth. The element itself is
              empty; the value of the element is stored in its state attribute, and the semantic associated with the value is typically in a specialized name derived from this element. Category:
              Specialization elements</a:documentation>
         <ref name="boolean.attlist"/>
         <ref name="boolean.content"/>
      </element>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron"
           name="boolean.attlist"
           combine="interleave">
      <ref name="boolean.attributes"/>
   </define>
   <a:documentation xmlns:sch="http://purl.oclc.org/dsdl/schematron">LONG NAME: State</a:documentation>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="state.content">
      <empty/>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="state.attributes">
      <attribute name="name"/>
      <attribute name="value"/>
      <ref name="univ-atts"/>
      <optional>
         <attribute name="outputclass"/>
      </optional>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="state.element">
      <element name="state" a:longName="State">
         <a:documentation>The &lt;state&gt; element specifies a name/value pair whenever it is necessary to represent a named state that has a variable value. The element is primarily intended for use
              in specializations to represent specific states (like logic circuit states, chemical reaction states, airplane instrumentation states, and so forth). Category: Specialization
              elements</a:documentation>
         <ref name="state.attlist"/>
         <ref name="state.content"/>
      </element>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron"
           name="state.attlist"
           combine="interleave">
      <ref name="state.attributes"/>
   </define>
   <a:documentation xmlns:sch="http://purl.oclc.org/dsdl/schematron">LONG NAME: Image Data</a:documentation>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="image.content">
      <optional>
         <ref name="alt"/>
      </optional>
      <optional>
         <ref name="longdescref"/>
      </optional>
   </define>
   <define xmlns:sch="http://purl.oclc.org/dsdl/schematron" name="image.attributes">
      <optional>
         <attribute name="href"/>
      </optional>
      <optional>
