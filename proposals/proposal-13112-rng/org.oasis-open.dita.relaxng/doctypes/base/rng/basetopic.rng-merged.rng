      </optional>
      <optional>
         <attribute name="type"/>
      </optional>
      <optional>
         <attribute name="scope">
            <choice>
               <value>external</value>
               <value>local</value>
               <value>peer</value>
               <value>-dita-use-conref-target</value>
            </choice>
         </attribute>
      </optional>
      <optional>
