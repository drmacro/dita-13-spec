default namespace m = "http://www.w3.org/1998/Math/MathML"
namespace local = ""

# redefine the anyElement to exclude elements in no namespace, that is DITA elements
include "mathml3/mathml3.rnc" {
    anyElement =
        element * - (m:* | local:*) {
            (attribute * { text }
             | text
             | anyElement)*
        }
}
